{
    "name": "${containerEnv:DEV_NAME}",
    "containerName": "${containerEnv:DEV_NAME}",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".",
    },
    // "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/${localWorkspaceFolderBasename},type=bind",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}/.devcontainer/${FOLDER_NAME}",
    "remoteUser": "root",
    "command": "sleep infinity",
    "forwardPorts": [8080],
    "shutdownAction": "stopCompose",
